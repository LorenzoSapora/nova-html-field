{
    "name": "yaroslawww/nova-html-field",
    "description": "HTML Laravel Nova field. (alternative of text field)",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ThinkStudio\\HtmlField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThinkStudio\\HtmlField\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
